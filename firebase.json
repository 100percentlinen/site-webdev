{
  "hosting": {
    "public": "publish",
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
      { "source": "/angular/quickstart", "destination": "/angular/tutorial/toh-pt0.html", "type": 301 },
      { "source": "/angular/guide/cheatsheet", "destination": "/angular/cheatsheet.html", "type": 301 },
      { "source": "/angular/guide/glossary", "destination": "/angular/glossary.html", "type": 301 },
      
      { "source": "/codelabs/angular2_components", "destination": "/codelabs/angular_components", "type": 301 },
      { "source": "/codelabs/angular2_components/1-base", "destination": "/codelabs/angular_components/1-base", "type": 301 },
      { "source": "/codelabs/angular2_components/2-easystart", "destination": "/codelabs/angular_components/2-easystart", "type": 301 },
      { "source": "/codelabs/angular2_components/3-usebuttons", "destination": "/codelabs/angular_components/3-usebuttons", "type": 301 },
      { "source": "/codelabs/angular2_components/4-final", "destination": "/codelabs/angular_components/4-final", "type": 301 },
      { "source": "/codelabs/angular2_components/what-next", "destination": "/codelabs/angular_components/what-next", "type": 301 }
    ],
    "rewrites": [
      { "source": "/examples/ng/doc/router/**", "destination": "/examples/ng/doc/router/index.html" },
      { "source": "/examples/ng/doc/toh-5/**", "destination": "/examples/ng/doc/toh-5/index.html" },
      { "source": "/examples/ng/doc/toh-6/**", "destination": "/examples/ng/doc/toh-6/index.html" },
      { "source": "/articles/low-level-html/improving-the-dom", "destination": "/guides/web-programming" }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
